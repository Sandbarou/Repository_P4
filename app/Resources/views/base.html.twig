<!DOCTYPE html>
<html>
    <head>
        
        <meta charset="UTF-8" />
        
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        {% block stylesheets %}
            <link href="{{ asset('bundles/oclouvre/css/materialize.css') }}" rel="stylesheet"/>
            <link href="{{ asset('bundles/oclouvre/css/materialize.min.css') }}" rel="stylesheet"/>
            <link href="{{ asset('bundles/oclouvre/css/style.css') }}" rel="stylesheet"/>     
        {% endblock %}
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
            <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
            <script type="text/javascript" src="{{ asset('bundles/oclouvre/js/init.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/oclouvre/js/materialize.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/oclouvre/js/materialize.js') }}"></script>
        {% endblock %}
    </body>
</html>
